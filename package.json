{
  "name": "react-roving-tabindex",
  "version": "0.7.0",
  "description": "React implementation of a roving tabindex",
  "author": "stevejay",
  "license": "MIT",
  "keywords": [
    "hooks",
    "react",
    "roving",
    "tab",
    "tabindex"
  ],
  "repository": "stevejay/react-roving-tabindex",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 jest --env=jsdom",
    "test:watch": "jest --env=jsdom --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "upgrade-interactive": "npm-check --update",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "eslint": "eslint --max-warnings 0 \"src/**/*.{js,jsx,ts,tsx}\"",
    "ts-check": "tsc --noEmit",
    "lint": "npm run eslint",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run ts-check"
    }
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts}": [
      "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
      "git add"
    ]
  },
  "dependencies": {
    "array-find-index": "^1.0.2",
    "lodash.uniqueid": "^4.0.1",
    "warning": "^4.0.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@storybook/storybook-deployer": "^2.8.1",
    "@svgr/rollup": "^4.3.3",
    "@testing-library/react": "^9.3.0",
    "@types/array-find-index": "^1.0.0",
    "@types/jest": "^24.0.19",
    "@types/jsdom": "^12.2.4",
    "@types/lodash.uniqueid": "^4.0.6",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "4.1.8",
    "@types/warning": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jsdom": "^15.2.0",
    "lint-staged": "^9.4.2",
    "lodash": "^4.17.15",
    "npm-check": "^5.9.0",
    "prettier": "1.18.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-hooks-testing-library": "^0.6.0",
    "react-test-renderer": "^16.11.0",
    "rollup": "^1.25.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.24.3",
    "rollup-plugin-url": "^3.0.0",
    "styled-components": "^4.4.0",
    "typescript": "^3.6.4"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
